<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{{ range . }}
    {{- if .IsExternalLink }}{{ continue }}{{ end }}
    <url>
        <loc>{{ .Permalink }}</loc>
        <lastmod>{{ .LastModified }}</lastmod>
        <priority>{{ .Priority }}</priority>
    </url>
{{ end }}
</urlset>
